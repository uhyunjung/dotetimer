#include <iostream>
#include <vector>
#include <string>
#include <algorithm>
using namespace std;

vector<string> word;                               // 단어 입력
int result = 0;                                    // 결과
int alpha[26];		                           // 자릿수

void cal() {
	for (int i = 0; i < word.size(); i++) {
		int pow = 1;
		for (int j = word[i].size() - 1; j >= 0; j--) {
			int val = word[i][j] - 'A';               // 알파벳 인덱스
			alpha[val] += pow;                        // ex) 100A => alpha[0]=100
			pow *= 10;                                // 자릿수 올림
		}
	}
	sort(alpha, alpha + 26);                                  // 오름차순 정렬
	int num = 9;
	for (int i = 25; i >= 0; i--) {
		if (alpha[i] == 0) break;
		result += (alpha[i] * num);
		num--;
	}
}

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
	int num;
	string str;
	cin >> num;
	for (int i = 0; i < num; i++) {
		cin >> str;
		word.push_back(str);
	}
	cal();
	cout << result;
	return 0;
}
