#include <iostream>
#include <cstdio>
#include <string>
#include <algorithm>
using namespace std;

string str1, str2;
int lcs[1001][1001];

int main() {
    ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    cin >> str1 >> str2;

    str1 = "0" + str1;
    str2 = "0" + str2;

    for (int i = 0; i < str1.size(); i++)
    {
        for (int j = 0; j < str2.size(); j++)
        {
            if ((i == 0) || (j == 0))
            {
                lcs[i][j] = 0;
                continue;
            }

            if (str1[i] == str2[j])
            {
                lcs[i][j] = lcs[i - 1][j - 1] + 1;
            }
            else {
                if (lcs[i - 1][j] > lcs[i][j - 1])
                {
                    lcs[i][j] = lcs[i - 1][j];
                }
                else
                {
                    lcs[i][j] = lcs[i][j - 1];
                }
            }
        }
    }
    cout << lcs[str1.size() - 1][str2.size() - 1];
    return 0;
}
